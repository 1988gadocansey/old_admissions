@inject TTU_CORE_ASP_ADMISSION_PORTAL.Services.IHelper form
@{ Layout = null;

    string[] forms = { "HND", "MATURE", "DIPLOMA", "CERTIFICATE", "DEGREE" };

}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <title>Takoradi Technical University - Print Preview</title>
    <link rel="stylesheet" href="~/assets/style.css">


    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel='stylesheet' type='text/css'>

    <link rel="icon" type="image/x-icon" href="https://apply.ttuportal.com/old/public/assets/img/logo.png" />


    <style type="text/css" media="print">

        #print {
            text-align: right;
        }

        .capitalize {
            text-transform: capitalize;
        }

        .ui.table td .uppercase {
            text-align: right;
            text-transform: uppercase;
        }

        @@media print {
            body {
                height: auto;
            }

            img {
                -webkit-print-color-adjust: exact;
            }

            #print {
                text-align: right;
            }

            .capitalize {
                text-transform: capitalize;
            }

            .ui.table td .uppercase {
                font-weight: bold;
                text-align: right;
                text-transform: uppercase;
            }

            table {
                float: none !important;
            }

            * {
                overflow: visible !important;
            }

            div {
                float: none !important;
            }

            #page1 {
                page-break-inside: avoid;
                page-break-after: always;
            }

            #page2 {
                page-break-inside: avoid;
                page-break-after: always;
            }

            #page3 {
                page-break-inside: avoid;
                page-break-before: always;
            }

            #page4 {
                page-break-inside: avoid;
                page-break-before: always;
            }

            .clear {
                clear: both;
            }
            /* clear here */
            table {
                page-break-inside: auto;
                border-collapse: collapse
            }

            tr {
                page-break-inside: avoid;
                page-break-after: auto
            }

            thead {
                display: table-header-group
            }

            tfoot {
                display: table-footer-group
            }
        }

        @@page {
            size: A4;
        }

        table, figure {
            page-break-inside: avoid;
        }

        fieldset legend {
            page-break-before: always;
        }

        h1, h2, h3, h4, h5 {
            page-break-after: avoid;
        }

        table {
            background: none;
        }
    </style>
</head>
<body id="dashboard" class="pushabless">
